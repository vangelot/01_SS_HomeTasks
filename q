[32m+[m[32m    files = 0[m
[32m+[m[32m    dirs = 0[m
[32m+[m[32m    for addr in os.listdir():[m
[32m+[m[32m        if os.path.isdir(addr):[m
[32m+[m[32m            dirs += 1[m
[32m+[m[32m            print('| ',addr)[m
[32m+[m[32m        else:[m
[32m+[m[32m            print(addr)[m
[32m+[m[32m            files += 1[m
[32m+[m
[32m+[m[32m    print('Files amonut: ',files,'\nDirectories amount: ',dirs)[m
