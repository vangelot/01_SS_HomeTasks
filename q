[32m+[m[32mdef remove_parentheses(s):[m
[32m+[m[32m    i = 0[m
[32m+[m[32m    res=''[m
[32m+[m[32m    while i < len(s):[m
[32m+[m[32m        if (s[i] == "(") and (")" in s[0:i]+s[i+1:]):[m
[32m+[m[32m            remove_parentheses(res + s[0:i]+s[i+1:])[m
[32m+[m[32m            #i += 1[m
