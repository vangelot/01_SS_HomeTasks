[32m+[m[32mdef cnk(n, k):[m
[32m+[m[32m    if k > n:[m
[32m+[m[32m        return 0[m
[32m+[m[32m    if k == 0:[m
[32m+[m[32m        return 1[m
[32m+[m[32m    else:[m
[32m+[m[32m        return cnk(n-1, k) + cnk(n-1,k-1)[m
